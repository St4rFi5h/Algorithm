import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int j ;

		j=scan.nextInt();


			for(int i=1; i<10 ;i++) {
				for(int a=1; a<=j*i;a++) {
					
				System.out.print("*");

			}
				System.out.println();
		}

	}



}




